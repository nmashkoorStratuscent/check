{
    "version": 1, "disable_existing_loggers": true,
    "loggers": {
        "root": {
            "handlers": ["standard_handler"],
            "level": "WARNING",
            "propagate": false
        },
        "data_quality_lib": {
            "handlers": ["standard_handler"],
            "level": "DEBUG",
            "propagate": false
        },
        "analyte_class": {
            "handlers": ["standard_handler"],
            "level": "DEBUG",
            "propagate": false
        }
    },
    "formatters": {
        "standard": {
            "format": "%(asctime)s [%(levelname)s] %(name)s: %(message)s"
        }
    },
    "handlers": {
        "standard_handler": {
            "level": "DEBUG",
            "formatter": "standard",
            "class": "logging.FileHandler",
            "filename": "log_path"
        }
    }
}
